<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/SecondActivityFragment">

    <fragment
        android:id="@+id/SecondActivityFragment"
        android:name="com.example.esandroidstudio.SecondActivityFragment"
        android:label="@string/second_activity_fragment_label"
        tools:layout="@layout/fragment_second_activity">

        <action
            android:id="@+id/action_SecondActivityFragment_to_FirstFragment"
            app:destination="@id/mainActivity"/>
    </fragment>

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.esandroidstudio.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>